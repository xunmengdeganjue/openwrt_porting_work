config interface loopback
        option ifname   lo
        option proto    static
        option ipaddr   127.0.0.1
        option netmask  255.0.0.0

config interface lan
        option ifname   eth0
        option proto    static
        option ipaddr   10.10.83.41
        option netmask  255.255.248.0
	option gateway	10.10.80.1
	option dns 	10.15.3.222
